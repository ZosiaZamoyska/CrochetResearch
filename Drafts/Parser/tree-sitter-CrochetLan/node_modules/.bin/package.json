{
  "name": "tree-sitter-crochetlan",
  "version": "0.0.1",
  "description": "CrochetLan grammar for tree-sitter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tree-sitter/tree-sitter-CrochetLan.git"
  },
  "license": "MIT",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "CrochetLan"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "dependencies": {
    "node-gyp-build": "^4.8.0",
    "node-addon-api": "^7.1.0"
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.22.6"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "build": "tree-sitter generate --no-bindings",
    "build-wasm": "tree-sitter build --wasm",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.CrochetLan",
      "injection-regex": "^CrochetLan$"
    }
  ],
  "directories": {
    "test": "test"
  },
  "author": "Zosia Marciniak",
  "bugs": {
    "url": "https://github.com/tree-sitter/tree-sitter-CrochetLan/issues"
  },
  "homepage": "https://github.com/tree-sitter/tree-sitter-CrochetLan#readme"
}
